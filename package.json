{
  "name": "mfe-service",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf ./dist",
    "compile": "tsc --build tsconfig.build.json",
    "dev": "npm run build && npm run start",
    "start": "node .",
    "serve": "nodemon --watch src -e ts --exec ts-node --esm ./src/index.ts",
    "test": "c8 mocha",
    "tdd": "nodemon --watch src -e ts --exec npm run test",
    "lint": "tsc --noEmit && eslint ./src --ext ts --quiet",
    "pretty-check": "prettier --check src/**/*.ts",
    "pretty-fix": "prettier --write src/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@decorators/di": "^3.1.0",
    "@decorators/express": "^3.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "sinon": "^15.2.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.3",
    "@types/sinon": "^10.0.16",
    "@types/sinon-chai": "^3.2.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "c8": "^8.0.1",
    "chai": "^4.3.7",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^5.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
